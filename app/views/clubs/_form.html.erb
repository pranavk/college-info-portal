    <%= f.label :pwd, "Password" %><br />
    <%= f.password_field :pwd %>
  </div>
  <div class="field">
    <%= f.label :contact %><br />
    <%= f.text_field :contact %>
  </div>
  <div class="field">
    <%= f.label :fcoordinator %><br />
